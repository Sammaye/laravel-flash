{
  "name": "sammaye/laravel-flash",
  "description": "My very Laravel first plugin, based on https://github.com/laracasts/flash",
  "license": "MIT",
  "authors": [
    {
      "name": "Sammaye",
      "homepage": "https://sammaye.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "~5.0"
  },
  "autoload": {
    "psr-0": {
      "sammaye\\Flash": ""
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "sammaye\\Flash\\FlashServiceProvider"
      ],
      "aliases": {
        "Flash": "sammaye\\Flash\\Flash"
      }
    }
  }
}
